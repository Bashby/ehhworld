{
  "name": "ehhworldclient",
  "version": "0.1.0",
  "description": "Client frontend logic for EhhWorld.",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env APP_VERSION=$npm_package_version webpack-serve --require babel-register --config ./webpack.dev.config.babel.js",
    "build": "webpack --config ./webpack.prod.config.babel.js --progress --profile --colors",
    "serve": "node build/app.js",
    "lint": "tslint ./app/**/*.{ts,tsx}",
    "lint:fix": "tslint ./app/**/*.{ts,tsx} --fix",
    "pretest": "npm run lint",
    "generate-protocol-buffers": "pbjs -t static-module -w es6 --es6 -o app/network/message.compiled.js app/network/message.proto && pbts -o app/network/message.compiled.d.ts app/network/message.compiled.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ehhio/ehhworldclient.git"
  },
  "keywords": [
    "react",
    "redux",
    "pixijs",
    "protobuf",
    "browser",
    "game"
  ],
  "author": "Bashby",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/ehhio/ehhworldclient#readme",
