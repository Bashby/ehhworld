{
  "name": "ehhworldclient",
  "version": "0.1.0",
  "description": "Client frontend logic for EhhWorld.",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --hot --watch-poll --host 0.0.0.0 --config ./webpack.dev.config.js --progress --colors",
    "build": "webpack --config ./webpack.prod.config.js --progress --profile --colors",
    "serve": "node build/app.js",
    "lint": "npm run lint:tslint && npm run lint:eslint",
    "lint:fix": "npm run lint:tslint:fix && npm run lint:eslint:fix",
    "lint:tslint": "tslint ./app/**/*.{ts,tsx}",
    "lint:tslint:fix": "tslint ./app/**/*.{ts,tsx} --fix",
    "lint:eslint": "eslint ./app/**",
    "lint:eslint:fix": "eslint ./app/** --fix",
    "pretest": "npm run lint",
    "generate-protocol-buffers": "pbjs -t static-module -w es6 --es6 -o app/network/message.compiled.js app/network/message.proto && pbts -o app/network/message.compiled.d.ts app/network/message.compiled.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ehhio/ehhworldclient.git"
  },
  "keywords": [
    "react",
    "redux",
    "pixijs",
    "protobuf",
    "browser",
    "game"
  ],
  "author": "Bashby",
  "license": "UNLICENSED",
  "homepage": "https://bitbucket.org/ehhio/ehhworldclient#readme",
