    "@types/react-router-redux": "^5.0.10",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^4.0.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
