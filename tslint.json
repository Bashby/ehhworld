{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "no-empty": false,
        "no-empty-interface": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "**/*.compiled.*"
        ]
    }
}